                style="-fx-background-color: #49b310;" text="Start Server" textFill="#f8f6f6"/>

        <ScrollPane fx:id="scrollpane" layoutX="14.0" layoutY="14.0">
            <TextArea fx:id="logTA" editable="false" layoutX="3.0" layoutY="5.0" prefHeight="260.0" prefWidth="485.0"/>
