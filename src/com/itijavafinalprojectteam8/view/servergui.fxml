        <ScrollPane layoutX="513.0" vbarPolicy="NEVER">
            <GridPane style="-fx-padding: 10px;" hgap="10" fx:id="playersGrid" disable="true" layoutX="520.0"
                      prefHeight="429.0" prefWidth="225.0">
            </GridPane>
        </ScrollPane>

    </children>
